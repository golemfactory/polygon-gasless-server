import { Buffer } from '../../npm:buffer';

/* eslint-disable @typescript-eslint/no-explicit-any */
const createBuffer = (value, encoding) => Buffer.from(value, encoding);

function defineCrc(model, calculator) {
    const result = (value, previous) => calculator(createBuffer(value), previous) >>> 0;
    result.signed = (value, previous) => calculator(createBuffer(value), previous);
    result.unsigned = result;
    result.model = model;
    return result;
}

export { defineCrc as d };

