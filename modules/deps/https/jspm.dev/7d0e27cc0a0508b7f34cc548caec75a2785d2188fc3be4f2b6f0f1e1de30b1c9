  (this || _global)._bh = (this || _global)._bh + bh + getCarry((this || _global)._bl, bl) | 0;
  (this || _global)._ch = (this || _global)._ch + ch + getCarry((this || _global)._cl, cl) | 0;
  (this || _global)._dh = (this || _global)._dh + dh + getCarry((this || _global)._dl, dl) | 0;
  (this || _global)._eh = (this || _global)._eh + eh + getCarry((this || _global)._el, el) | 0;
  (this || _global)._fh = (this || _global)._fh + fh + getCarry((this || _global)._fl, fl) | 0;
  (this || _global)._gh = (this || _global)._gh + gh + getCarry((this || _global)._gl, gl) | 0;
  (this || _global)._hh = (this || _global)._hh + hh + getCarry((this || _global)._hl, hl) | 0;
