import { keccak_224, keccak_256, keccak_384, keccak_512 } from '../npm:@noble/hashes@1.3.1/sha3';
import { wrapHash } from './utils';
import '../npm:@noble/hashes@1.3.1/_assert';
import '../npm:@noble/hashes@1.3.1/utils';

const keccak224 = wrapHash(keccak_224);
const keccak256 = (() => {
    const k = wrapHash(keccak_256);
    k.create = keccak_256.create;
    return k;
})();
const keccak384 = wrapHash(keccak_384);
const keccak512 = wrapHash(keccak_512);

export { keccak224, keccak256, keccak384, keccak512 };

