var exports = {};
exports = assert;

function assert(val, msg) {
  if (!val) throw new Error(msg || "Assertion failed");
}

assert.equal = function assertEqual(l, r, msg) {
  if (l != r) throw new Error(msg || "Assertion failed: " + l + " != " + r);
};

var exports$1 = exports;

export default exports$1;

